{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/deno@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api/index.ts"
    },
    {
      "src": "/meta.discover",
      "dest": "/api/index.ts"
    },
    {
      "src": "/mcp",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
